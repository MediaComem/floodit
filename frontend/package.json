{
  "name": "floodit-frontend",
  "version": "0.0.0",
  "description": "Flood It frontend",
  "license": "MIT",
  "author": "Simon Oulevay <npm@alphahydrae.com>",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=8048 ng build",
    "check": "npm run format",
    "check:write": "npm run format:write",
    "format": "prettier --config ../.prettierrc.json --check .",
    "format:write": "prettier --config ../.prettierrc.json --write .",
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=8048 ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@types/luxon": "^3.0.0",
    "daisyui": "^2.31.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.0.1",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular/cli": "^17.3.11",
    "@angular/compiler-cli": "^17.3.12",
    "@types/lodash-es": "^4.17.6",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.16",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.1.8",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 7"
  }
}
